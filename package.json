{
  "name": "ember-yeti-table",
  "version": "1.7.3",
  "description": "Lightweight and flexible tables addon.",
  "keywords": [
    "ember-addon",
    "table",
    "tables",
    "datagrid",
    "yeti"
  ],
  "homepage": "https://miguelcobain.github.io/ember-yeti-table",
  "repository": "https://github.com/miguelcobain/ember-yeti-table",
  "license": "MIT",
  "author": "Miguel Andrade <jmandrade0@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "eslint-config-check": "eslint --print-config . | eslint-config-prettier-check",
    "format:all": "run-s format:prettier format:eslint",
    "format:eslint": "eslint --fix \"**/*.js\"",
    "format:prettier": "prettier --write \"**/*.{js,scss,json,yaml,yml}\"",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,gjs,scss,json,yaml,yml,md}": "prettier --write",
    "*.js,gjs": "eslint --fix"
  },
  "resolutions": {
    "ember-cli-clipboard": "0.13.0"
  },
  "dependencies": {
    "deepmerge": "^4.3.0",
    "ember-async-data": "^1.0.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-modifier": "^4.1.0",
    "ember-resources": "^6.4.0",
    "ember-template-imports": "^3.4.2",
    "tracked-toolbox": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@ember/optional-features": "^1.3.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@glint/template": "1.0.0-beta.3",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "^3.0.1",
    "concurrently": "^8.0.1",
    "ember-cli": "~4.12.2",
    "ember-cli-addon-docs": "^5.2.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.3.7",
    "ember-concurrency-decorators": "^2.0.3",
    "ember-data": "~4.12.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "faker": "^5.5.3",
    "husky": "^8.0.0",
    "lint-staged": "^10.5.4",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.2",
    "prettier-plugin-ember-template-tag": "^1.0.2",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sinon": "^9.2.4",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-prettier": "^3.0.0",
    "webpack": "^5.78.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://miguelcobain.github.io/ember-yeti-table"
  }
}
